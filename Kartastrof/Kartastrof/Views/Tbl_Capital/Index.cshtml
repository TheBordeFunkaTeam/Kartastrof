@model IEnumerable<Kartastrof.Models.Tbl_Capital>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<h3>Fyll databas från fil (ange filens sökväg)</h3>
@using (Html.BeginForm("FillDatabase", "Tbl_Capital")) { 
<input id="Text1" type="text" name="fileDest"/>
<input id="Submit1" type="submit" value="Fyll databas" name="fileDest" />
}

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Ca_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ca_Longitude)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ca_Latitude)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Ca_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ca_Longitude)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ca_Latitude)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Ca_Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Ca_Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Ca_Id })
        </td>
    </tr>
}

</table>
